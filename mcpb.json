{
    "name": "openclaw-molt-mcp",
    "version": "0.1.0",
    "description": "MCP server for OpenClaw/Moltbook ecosystem integration",
    "author": "Sandraschi",
    "license": "MIT",
    "mcp_version": "1.0.0",
    "capabilities": {
        "tools": true,
        "prompts": true
    },
    "dependencies": {
        "python": ">=3.11",
        "fastmcp": ">=0.4.1",
        "httpx": ">=0.27.0",
        "pydantic": ">=2.0",
        "pydantic-settings": ">=2.0"
    },
    "build": {
        "include": [
            "src/",
            "mcpb/manifest.json",
            "README.md",
            "LICENSE",
            "SECURITY.md",
            "docs/"
        ],
        "exclude": [
            "**/__pycache__",
            "tests/",
            "webapp/",
            "webapp_api/",
            ".venv/",
            "dist/",
            ".cursor/",
            "*.mcpb"
        ]
    }
}